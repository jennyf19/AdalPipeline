@model AdalPipeline.Models.Request

@{
    ViewBag.Title = "CreateRequest";
    Layout = "/Views/Shared/_Layout.cshtml";
    Html.EnableClientValidation(false);
}

@using System.Web.UI.WebControls

<h2>@Html.LabelForModel()</h2>

@using (Html.BeginForm())
{
    @Html.ValidationSummary()
    <p>Your name: @Html.EditorFor(m => m.AuthorName)</p>
    <p>Release name: @Html.EditorFor(m => m.ReleaseTitle)</p>
    <p>Version number: @Html.EditorFor(m => m.ReleaseVersion)</p>
    <p>Release notes: @Html.EditorFor(m => m.ReleaseNotes)</p>
    <input type="submit" value="OK" />
}
